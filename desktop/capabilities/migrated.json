        }
      ]
    },
    "shell:allow-open",
    "dialog:allow-open",
    "dialog:allow-save",
    "notification:default",
    "os:allow-platform",
    "os:allow-version",
    "os:allow-os-type",
    "os:allow-family",
    "os:allow-arch",
    "os:allow-exe-extension",
    "os:allow-locale",
    "os:allow-hostname",
    "process:allow-restart",
    "process:allow-exit",
    "clipboard-manager:allow-write-text",
    "os:default",
    "dialog:default",
    "process:default",
    "fs:default",
    "notification:default"
  ]
}
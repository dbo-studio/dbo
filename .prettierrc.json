{
  "plugins": ["prettier-plugin-organize-imports"],
  "printWidth": 120,
  "tabWidth": 2,
  "useTabs": false,
  "trailingComma": "none",
  "bracketSpacing": true,
  "semi": true,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "overrides": [
  {
    "files": ["*.ts", "*.tsx"],
    "options": {
      "parser": "typescript"
    }
  }
  ]
}
